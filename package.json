{
  "name": "integra-ai",
  "version": "1.0.0",
  "description": "A TypeScript GenAI application using Hugging Face models",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts --watch",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "echo \"Tests not yet implemented. Run 'npm run dev' to test the application manually.\"",
    "test:app": "node dist/index.js || echo \"Build the app first with 'npm run build'\""
  },
  "keywords": [
    "ai",
    "typescript",
    "huggingface",
    "genai",
    "text-generation"
  ],
  "author": "M1ztick",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.0.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.5.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
